les\";\nimport { Box, Card, Button, CircularProgress } from \"@material-ui/core\";\n\nimport TextField from \"@material-ui/core/TextField\";\n\nconst useStyles = makeStyles({\n  container: {},\n  title: {\n    fontWeight: \"600\",\n    fontSize: \"20px\",\n  },\n  formContainer: {\n    padding: \"20px\",\n    display: \"flex\",\n    flexDirection: \"column\",\n    minWidth: \"500px\",\n  },\n  btn: {\n    height: \"40px\",\n    background: \"rgba(16, 185, 129,0.3)\",\n    border: \"1px solid rgba(16, 185, 129)\",\n    marginTop: \"20px\",\n    \"&:hover\": {\n      background: \"rgba(16, 185, 129,0.3)\",\n    },\n    color: \"rgba(16, 185, 129)\",\n    textTransform: \"capitalize\",\n    padding: \"20px 50px 20px 50px\",\n  },\n});\n\nfunction Payment() {\n  const classes = useStyles();\n\n  return (\n    <Box className={classes.container}>\n      <label className={classes.title}>How do you want to pay?</label>\n      <Box>\n        <label\n          style={{\n            background: \"#e1f4f5\",\n            display: \"flex\",\n            padding: \"15px\",\n            color: \"#6969fd\",\n            fontWeight: \"500\",\n          }}\n        >\n          {\" \"}\n          Credit Card\n        </label>\n      </Box>\n      <form>\n        <Card className={classes.formContainer}>\n          <label\n            style={{ display: \"flex\", color: \"#6969fd\", fontWeight: \"500\" }}\n          >\n            Credit Card Number\n          </label>\n          <TextField\n            id=\"outlined-basic\"\n            label=\"Name\"\n            name=\"name\"\n            variant=\"outlined\"\n            style={{ marginBottom: \"20px\" }}\n          />\n          <Box\n            mb=\"15px\"\n            style={{\n              display: \"flex\",\n              justifyContent: \"space-between\",\n              alignItems: \"center\",\n            }}\n          >\n            <div>\n              <label style={{ color: \"#6969fd\", fontWeight: \"500\" }}>\n         