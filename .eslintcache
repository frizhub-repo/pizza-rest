aurant } = useRestaurantConetxt();\n    console.log({ token });\n    const logout = () => {\n        window.localStorage.removeItem(\"token\");\n        window.location.reload();\n    };\n    // React.useEffect(() => {\n    //     setToken(localStorage.getItem(\"token\"));\n    // });\n\n    const [anchorEl, setAnchorEl] = React.useState(null);\n\n    const handleClick = (event) => {\n        setAnchorEl(event.currentTarget);\n    };\n\n    const handleClose = () => {\n        setAnchorEl(null);\n    };\n\n    return (\n        <header className=\"text-gray-700 body-font \">\n            <div\n                className={`flex w-full  justify-content-between px-44 py-4 ${classes.shadow}`}\n            >\n                <div className=\"w-25\">\n                    <img\n                        style={{ height: \"50px\" }}\n                        src={`${process.env.REACT_APP_API_BASE_URL}/${restaurant?.restaurant?.logoUrl}`}\n                        className=\"object-cover\"\n                    />\n                </div>\n                <div className=\"flex-grow-1 d-flex justify-content-end\">\n                    {!token ? (\n                        <button\n                            className=\"  bg-yellow-500 text-yellow-500 bg-opacity-50 border-2 border-yellow-500\t py-1 px-3   text-xs font-weight-bold\"\n                            onClick={() => setModalShow(true)}\n                        >\n                            Login/Signup\n                        </button>\n                    ) : (\n                        <div style={{ display: \"flex\", alignItems: \"center\" }}>\n                            <AccountCircleIcon\n                                onClick={handleClick}\n                                fontSize=\"large\"\n                            />\n                        </div>\n                    )}\n                    <CustomMenu\n                        handleClose={handleClose}\n                        anchorEl={anchorEl}\n          